    <pair geom1="g_index_proximal" geom2="g_middle_mediate" condim="1"></pair>

    <pair geom1="g_index_distal" geom2="g_middle_mediate" condim="1"></pair>
    <pair geom1="g_index_distal" geom2="g_middle_distal" condim="1"></pair>
    <pair geom1="g_index_mediate" geom2="g_middle_distal" condim="1"></pair>

    <pair geom1="g_index_proximal" geom2="g_middle_distal" condim="1"></pair>
    <pair geom1="g_index_distal" geom2="g_middle_proximal" condim="1"></pair> 

